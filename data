what_and_who = {'key0': [],}
inf = {'key0':['name','surname','email']}

def key_by_num(key, what):
    for i in what_and_who:
        if i == key:
            what_and_who[i].append(what)

    print(what_and_who)
def reg(key,name,surname,email):
    inf[key] = []
    inf[key].append(name)
    inf[key].append(surname)
    inf[key].append(email)
    what_and_who[key] = []



